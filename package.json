{
  "name": "vue-example",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "main": "./src/index.js",
  "config": {
    "env": "development",
    "serverHost": "127.0.0.1",
    "serverPort": 4444,
    "publicPath": "/"
  },
  "scripts": {
    "build": "npm run build:client; npm run build:server;",
    "build:client": "node ./src/scripts/build-client.js",
    "build:server": "node ./src/scripts/build-server.js",
    "start": "node ./src/scripts/start"
  },
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "dependencies": {
    "express": "5.0.0-alpha.2",
    "express-vue-builder": "2.10.1",
    "vue": "2.0.5",
    "vue-builder": "1.5.2",
    "vue-router": "2.0.1",
    "vue-server-renderer": "2.0.5"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-3": "6.17.0",
    "css-loader": "0.25.0",
    "express-vue-dev": "1.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "inline-environment-variables-webpack-plugin": "1.1.0",
    "postcss-cssnext": "2.8.0",
    "vue-loader": "9.9.0",
    "vue-webpack": "1.7.1",
    "webpack": "2.1.0-beta.25",
    "webpack-hot-middleware": "2.13.2",
    "webpack-manifest-plugin": "1.1.0"
  }
}
